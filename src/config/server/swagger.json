{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "RUPTURE API",
    "description": ""
  },
  "host": "localhost:3000",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "paths": {
    "/rupture/shopper/suggestion": {
      "get": {
        "description": "Search for product suggestions similar to a missing product",
        "parameters": [ 
           {
             "name": "plu",
             "in": "query",
             "description": "Product ID",
             "required": true,
             "type": "number"
           }
         ],
        "responses": {
          "200": {
            "description": "A list of products"
          },
          "400": {
            "description": "Input parameter errors or product ID not found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  }
}